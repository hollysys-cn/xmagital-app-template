generator client {
  provider = "prisma-client-js"
}

generator pothos {
  provider = "prisma-pothos-types"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Student {
  id             String   @id @default(cuid())
  name           String
  age            Int
  gender         String
  enrollmentDate DateTime
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}



